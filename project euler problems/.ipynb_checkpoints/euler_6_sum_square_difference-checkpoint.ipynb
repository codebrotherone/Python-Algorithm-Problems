    "def sum_square_diff(n):\n",
    "    x = (x**2 for x in range(1, n+1))\n",
    "    y = (x for x in range(n, -1, -1))\n",
    "    return sum(y)**2 - sum(x)\n",
    "    "
   ]
  },
